<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="100"
        x:Class="ThemeMii.ThemeMii_ckInput"
        Width="300" Height="100"
        Title="Input 45e"
        Loaded="ThemeMii_ckInput_Load">
        <StackPanel HorizontalAlignment="Center">
                <TextBlock Margin="5">
                        Type 45e to create common-key.bin
                </TextBlock>
                <TextBox Name="BoxInput"/>
                <Grid ColumnDefinitions="Auto,Auto" RowDefinitions="30" Margin="5">
                        <Button Click="btnOK_Click">OK</Button>
                        <Button Grid.Column="1" Click="btnCancel_Click">Cancel</Button>
                </Grid>
        </StackPanel>
</Window>
